
public class CountSetBits {
	public static void main(String[] args) {
		System.out.println("100 : " + countSetBitsNaive(100));
		System.out.println("100 : " + countSetBitsNaive(100));
		System.out.println("100 : " + countSetBitsNaive(100));
		System.out.println("100 : " + countSetBitsNaive(100));
	}

	private static int countSetBitsNaive(int n) {
		int cnt = 0;
		while(n != 0) {
			cnt += n%2;
			n /= 2;
		}
		return cnt;
	}
}
