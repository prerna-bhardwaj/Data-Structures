
public class kthBitSet {
	public static void main(String[] args) {
		System.out.println(kthBitSetNaive(100, 3));
		System.out.println(usingLeftShift(100, 3));
		System.out.println(usingRightShift(100, 3));
	}

	private static boolean kthBitSetNaive(int n, int k) {
		int cnt = 1;
		while(cnt < k) {
			n /= 2;
			cnt++;
		}
		return n%2 == 1;
	}
}
