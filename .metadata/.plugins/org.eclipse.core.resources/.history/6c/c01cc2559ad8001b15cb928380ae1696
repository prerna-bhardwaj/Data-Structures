
public class kthBitSet {
	public static void main(String[] args) {
		System.out.println(kthBitSetNaive(8, 3));
		System.out.println(usingLeftShift(100, 3));
		System.out.println(usingRightShift(100, 3));
	}

	private static char[] usingRightShift(int i, int j) {
		// TODO Auto-generated method stub
		return null;
	}

	private static char[] usingLeftShift(int i, int j) {
		// TODO Auto-generated method stub
		return null;
	}

	private static boolean kthBitSetNaive(int n, int k) {
		int cnt = 1;
		while(cnt < k) {
			n /= 2;
			cnt++;
		}
		return n%2 == 1;
	}
}
